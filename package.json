{
	"name": "load-tui",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "bin/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc && chmod u+x ./bin/cli.js"
	},
	"files": [
		"bin"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"meow": "^12.0.1",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/ink": "^2.0.3",
		"@types/meow": "^6.0.0",
		"@types/react": "^18.2.6",
		"@vdemedes/prettier-config": "^2.0.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
